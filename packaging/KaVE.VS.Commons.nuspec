<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>KaVE.VS.Commons</id>
        <version>$version$</version>

        <description>Common interfaces of the KaVE project that are relevant for plugins that extend FeedBaG.</description>
        <authors>KaVE Team</authors>
        <copyright>Copyright 2011-2017</copyright>
    
        <projectUrl>http://kave.cc/</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/kave-cc/resources/master/images/logo-nuget.png</iconUrl>
        <licenseUrl>https://github.com/kave-cc/resources/blob/master/LICENSE</licenseUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>

        <dependencies>
            <!-- solution references -->
            <dependency id="KaVE.Commons" version="$version$" />
            <!-- from packages.config -->
            <dependency id="EnvDTE" version="8.0.1" />
            <dependency id="Microsoft.VisualStudio.CommandBars" version="8.0.0.0" />
            <dependency id="JetBrains.ReSharper.SDK" version="2018.1.0" />
            <!-- from 3rd-party/non-NuGet? we have to repackage them in the files section -->
        </dependencies>
    </metadata>
    <files>
        <file src="..\KaVE.VS.Commons\bin\Debug\KaVE.VS.Commons.dll" target="lib\" />
        <file src="install.ps1" target="tools\" />
    </files>
</package>